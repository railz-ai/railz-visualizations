{
  "name": "railz-visualizations",
  "version": "0.0.19",
  "description": "Railz.ai Visualizations",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencilcomponent/stencilcomponent.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:prod": "stencil build --docs --prod",
    "build:stage": "stencil build --docs --stage",
    "build:qa2": "stencil build --docs --qa2",
    "build:qa": "stencil build --docs --qa",
    "build": "stencil build --docs --local",
    "start:prod": "stencil build --prod --watch --serve",
    "start:stage": "stencil build --stage --watch --serve",
    "start:qa2": "stencil build --qa2 --watch --serve",
    "start:qa": "stencil build --qa --watch --serve",
    "start": "stencil build --local --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "2.7.0",
    "@stencil/sass": "1.5.2",
    "date-fns": "2.28.0",
    "highcharts": "9.3.2",
    "lodash": "4.17.21",
    "numbro": "2.3.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "^17.0.16",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "puppeteer": "10.0.0",
    "rollup-plugin-dotenv": "^0.3.0"
  },
  "license": "MIT"
}
